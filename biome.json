{
	"$schema": "https://biomejs.dev/schemas/1.3.3/schema.json",
	"organizeImports": {
		"enabled": true,
		"ignore": [
			"node_modules/*",
			"*/build/*",
			"src/data.ts",
			"src/data.example.ts"
		]
	},
	"formatter": {
		"ignore": [
			"node_modules/*",
			"*/build/*",
			"src/data.ts",
			"src/data.example.ts"
		]
	},
	"linter": {
		"ignore": [
			"node_modules/*",
			"*/build/*",
			"src/data.ts",
			"src/data.example.ts"
		],
		"enabled": true,
		"rules": {
			"recommended": true
		}
	}
}
